#!/bin/bash
#Generated by JetBrains Toolbox 2.8.1.52155 at 2025-09-11T08:13:43

declare -a intellij_args=()
declare -- wait=""

for o in "$@"; do
  if [[ "$o" = "--wait" || "$o" = "-w" ]]; then
    wait="-W"
    o="--wait"
  fi
  if [[ "$o" =~ " " ]]; then
    intellij_args+=("\"$o\"")
  else
    intellij_args+=("$o")
  fi
done

open -na "/Applications/WebStorm.app/Contents/MacOS/webstorm" $wait --args "${intellij_args[@]}"