#!/bin/bash
#Generated by JetBrains Toolbox 2.8.1.52155 at 2025-09-10T11:13:30

declare -a intellij_args=()
declare -- wait=""

for o in "$@"; do
  if [[ "$o" = "--wait" || "$o" = "-w" ]]; then
    wait="-W"
    o="--wait"
  fi
  if [[ "$o" =~ " " ]]; then
    intellij_args+=("\"$o\"")
  else
    intellij_args+=("$o")
  fi
done

open -na "/Applications/PyCharm.app/Contents/MacOS/pycharm" $wait --args "${intellij_args[@]}"